<h1> Log In here!</h1>

<%= form_for @user, url: sessions_path do |f| %>

    <% if flash[:notice] %>
        <div class="notice"><%= flash[:notice] %><br></br></div>
    <% end %>

    <%= f.label :username %> <br> 
    <%= f.text_field :username %> <br></br> 

    <%= f.label :password %> <br>
    <%= f.password_field :password %> <br></br>

    <%= f.submit "Log In" %>
<% end %>


